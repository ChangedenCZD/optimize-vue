{
  "name": "optimize-vue",
  "version": "0.2.2",
  "description": "基于 vue-cli 3.0 构建的快速开发框架",
  "author": {
    "name": "changeden"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "clone": "latest",
    "co": "latest",
    "compatible-storage": "^0.1.6",
    "isarray": "^2.0.4",
    "md5": "^2.2.1",
    "node-sass": "4.11.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "vue": "2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.2",
    "@vue/cli-plugin-eslint": "^3.2.2",
    "@vue/cli-service": "^3.2.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/ChangedenCZD/optimize-vue/issues"
  },
  "homepage": "https://github.com/ChangedenCZD/optimize-vue#README",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChangedenCZD/optimize-vue.git"
  }
}
